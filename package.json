{
	"author": "sparrowhawk-ea",
	"bugs": {
		"url": "https://github.com/sparrowhawk-ea/fmm-vue/issues"
	},
	"description": "Form Minimap Vue",
	"devDependencies": {
		"@babel/types": "*",
		"@eafmm/demo": "*",
		"@mdi/font": "*",
		"@rollup/plugin-commonjs": "*",
		"@rollup/plugin-node-resolve": "*",
		"@rollup/plugin-replace": "*",
		"@types/vuelidate": "0",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"@vue/compiler-sfc": "3",
		"@vue/eslint-config-prettier": "6",
		"@vue/eslint-config-typescript": "7",
		"@vuelidate/core": "^2.0.0-alpha.27",
		"@vuelidate/validators": "^2.0.0-alpha.23",
		"eslint": "*",
		"eslint-plugin-prettier": "*",
		"eslint-plugin-vue": "7",
		"prettier": "*",
		"rollup": "*",
		"rollup-plugin-dts": "*",
		"rollup-plugin-esbuild": "*",
		"rollup-plugin-import-css": "*",
		"typescript": "*",
		"vue": "3",
		"vuetify": "^3.0.0-alpha.11",
		"vuex": "4"
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/sparrowhawk-ea/fmm-vue#readme",
	"keywords": [
		"Form",
		"Minimap",
		"Vue"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"name": "@eafmm/vue",
	"peerDependencies": {
		"@eafmm/core": "4"
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"printWidth": 132,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sparrowhawk-ea/fmm-vue.git"
	},
	"scripts": {
		"build": "rm -rf demo dist && npm run lint && rollup -c && npm run gzip",
		"gzip": "gzip -9 -c dist/index.js > dist/index.js.gz && gzip -9 -c dist/index.mjs > dist/index.mjs.gz && gzip -9 -c demo/index.js > demo/index.js.gz",
		"lint": "eslint --report-unused-disable-directives --ext .ts .",
		"prepare": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"typings": "dist/index.d.ts",
	"version": "5.0.0"
}

{
	"author": "sparrowhawk-ea",
	"bugs": {
		"url": "https://github.com/sparrowhawk-ea/fmm-vue/issues"
	},
	"description": "Form Minimap Vue",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"@vue/eslint-config-prettier": "6",
		"@vue/eslint-config-typescript": "7",
		"eslint": "*",
		"eslint-plugin-prettier": "*",
		"eslint-plugin-vue": "7",
		"prettier": "*",
		"tslib": "*",
		"typescript": "*",
		"vue": "3"
	},
	"files": [
		"dist/**/*"
	],
	"homepage": "https://github.com/sparrowhawk-ea/fmm-vue#readme",
	"keywords": [
		"Form",
		"Minimap",
		"Vue"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"name": "@eafmm/vue",
	"peerDependencies": {
		"@eafmm/core": "3"
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"printWidth": 132,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sparrowhawk-ea/fmm-vue.git"
	},
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "eslint --report-unused-disable-directives --ext .ts .",
		"postversion": "git push && git push --tags",
		"prebuild": "rm -rf dist",
		"prepare": "npm run build",
		"prepublishOnly": "npm run lint",
		"preversion": "npm run lint",
		"test": "echo \"Error: no test specified\" && exit 1",
		"version": "npm run format && git add -A src"
	},
	"types": "dist/index.d.ts",
	"version": "4.0.1"
}
